@{
    ViewData["Title"] = "Services";
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - DhakaChoiceLtd.</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.5.0/font/bootstrap-icons.css" integrity="sha384-tKLJeE1ALTUwtXlaGjJYM3sejfssWdAaWR2s97axw4xkiAdMzQjtOjgcyw0Y50KU" crossorigin="anonymous">
    <link rel="stylesheet" href="~/css/site.css" />
    <link href="~/css/Doctor.css" rel="stylesheet" />
    <script src='https://kit.fontawesome.com/a076d05399.js' crossorigin='anonymous'></script>
</head>
<body>
    <header>
        <nav style="" class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
            <div class="container">
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex flex-sm-row-reverse" style="margin-right: -50px;">
                    <partial name="_LoginPartial" />
                    <ul class="navbar-nav">
                        <li class="nav-item">
                            <a id="nav-item-a" class="nav-link text-dark" asp-area="" asp-controller="Doctor" asp-action="DoctorMainPage"><b>Home</b></a>
                        </li>
                        <li class="nav-item">
                            <div class="dropdown">
                                <button class="dropbtn" style="border: none; outline: none;">
                                    Service
                                    <i class="fa fa-caret-down"></i>
                                </button>
                                <div class="dropdown-content">
                                    <a asp-area="" asp-controller="Doctor" asp-action="DoctorService">Doctor Service</a>
                                    <a href="#">Medicine Service</a>
                                    <a href="#">Ambulance Service</a>
                                </div>
                            </div>
                        </li>
                        <li class="nav-item">
                            <div class="dropdown">
                                <button class="dropbtn" style="border: none; outline: none;">
                                    Login
                                    <i class="fa fa-caret-down"></i>
                                </button>
                                <div class="dropdown-content">
                                    <a asp-area="" asp-controller="Doctor" asp-action="DoctorDashboard">As a Doctor</a>
                                    <a href="#">As a Patient</a>
                                </div>
                            </div>
                        </li>
                        <li class="nav-item">
                            <div class="dropdown">
                                <button class="dropbtn" style="border: none; outline: none;">
                                    Registration
                                    <i class="fa fa-caret-down"></i>
                                </button>
                                <div class="dropdown-content">
                                    <a asp-area="" asp-controller="Registration" asp-action="DoctorSignUp">As a Doctor</a>
                                    <a href="#">As a Patient</a>
                                </div>
                            </div>
                        </li>
                    </ul>
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <a class="nav-link text-danger font-weight-bold h5" asp-area="" asp-controller="Home" asp-action="Index">
                                <img src="~/Images/dhaka-choice-2f.png" style="width: 110px; height: 70px" class="fa-cog fa-5x slow-spin" id="Logo" />
                            </a>
                        </li>
                    </ul>
                </div>
                <ul class="navbar-nav">
                    <li class="nav-item">
                        <a class="nav-link text-danger font-weight-bold h5" asp-area="" asp-controller="Doctor" asp-action="DoctorMainPage" id="mobilescreenLogo">Dhaka Choice Ltd.</a>
                    </li>
                </ul>
            </div>
        </nav>
    </header>
    <div class="container">
        <main role="main" class="indexfirstDiv">
            @RenderBody()
        </main>
    </div>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @*<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>*@
    @*<script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.16.0/umd/popper.min.js"></script>*@
    @*<script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js"></script>*@


    @await RenderSectionAsync("Scripts", required: false)
<script>
    $('.nav li').click(function(){
        $('.nav li').removeClass('active');
        $(this).addClass('active');
    })
</script>

</body>
</html>
