<style>
    .card-body {
        padding: 1.25rem;
    }

    /*statistics box style*/
    .card-header-icon {
        margin-right: 10px;
        font-size: 18px;
    }

    .card-header-panel {
        background-color: #263543 !important;
        color: white;
    }

        .card-header-panel h4 {
            margin-bottom: 0;
            /*  color: #333;*/
            font-size: 17px;
            font-weight: 700;
            display: inline-block;
            margin-right: 10px;
            line-height: 1.1;
            position: relative;
            color: white !important;
        }

        .card-header-panel h4 {
            color: white !important;
        }

    .card-body-icon {
        height: 70px;
        /*height: 100%;*/
        /*padding-left: 5px;*/
        /*padding-right: 5px;*/
    }

    .total-order {
        height: 70px;
    }

    /*last 7 days style*/
    .rm-pending {
        padding: 0;
    }

    .order-tab a {
        color: #4389e9;
        padding: 15px 0;
        text-align: center;
        width: 100%;
        font-size: 15px;
        font-weight: bold;
        display: inline-block;
    }

    .bg-order-tab a {
        color: white;
    }

    .order_list {
        padding: 0;
    }

    .order_status span {
        padding-right: 20px;
    }

    .order_right .rm-pending {
        padding: 2px 0;
    }

    .order_right h4 {
        margin: 0;
    }

    .total-earning {
        /*max-width: 100px;*/
        /*overflow-wrap: break-word;*/
        word-break: break-word;
    }
    #icon {
        box-shadow: 1px 8px 16px 0px rgba(0,0,0,0.7);
        height: 40px;
        width: 40px;
    }

        #icon:hover {
            transition: 0.2s ease all;
            box-shadow: 0 0px 0px 0 rgba(0,0,0,0);
            height: 39px;
            width: 39px;
        }

</style>


